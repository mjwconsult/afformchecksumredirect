<?xml version="1.0"?>
<extension key="afformchecksumredirect" type="module">
  <file>afformchecksumredirect</file>
  <name>FormBuilder Checksum Redirect</name>
  <description>Redirect FormBuilder forms to contribution pages etc. using checksum on submit</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Matthew Wire</author>
    <email>mjw@mjwconsult.co.uk</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/mjwconsult/afformchecksumredirect</url>
    <url desc="Support">https://mjw.pt/support/afformchecksumredirect</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2026-02-03</releaseDate>
  <version>1.1.0</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>6.4</ver>
  </compatibility>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Afformchecksumredirect</namespace>
    <format>25.10.2</format>
    <angularModule>crmAfformchecksumredirect</angularModule>
  </civix>
  <mixins>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>mgd-php@2.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>setting-admin@1.0.1</mixin>
    <mixin>scan-classes@1.0.0</mixin>
  </mixins>
</extension>
